   <children>
      <Label styleClass="titulos" text="CLOUD COMBAT">
         <font>
            <Font size="51.0" />
         </font>
         <padding>
            <Insets bottom="50.0" />
         </padding>
      </Label>
      <Button mnemonicParsing="false" onAction="#onStartGameAction" styleClass="botones" text="START GAME" />
      <Button mnemonicParsing="false" onAction="#onSettingsAction" styleClass="botones" text="SETTINGS" />
      <Button mnemonicParsing="false" onAction="#onExitAction" styleClass="botones" text="EXIT" />
